<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:agui="http://schemas.agui.com/apk/res/agui"
    package="${package}"
    agui:versionCode="1"
    agui:versionName="1.0">
    
    <application
        agui:icon="@drawable/ic_launcher">
        <activity
            agui:name=".${mainActivityName}"
            agui:label="@string/app_name">
            <intent-filter>
                <action agui:name="agui.intent.action.MAIN" />
                <category agui:name="agui.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>